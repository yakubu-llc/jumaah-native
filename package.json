{
  "name": "jumaah-native",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "codegen": "dotenv -e .env.local -- cross-var openapi-rq -i %EXPO_PUBLIC_BACKEND_API_URL%/openapi.yaml -c axios -o ./lib/sdk"
  },
  "dependencies": {
    "@7nohe/openapi-react-query-codegen": "^1.6.0",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.1.7",
    "@rn-primitives/dialog": "^1.0.4",
    "@rn-primitives/dropdown-menu": "^1.0.3",
    "@rn-primitives/slot": "^1.0.4",
    "@rn-primitives/types": "^1.0.4",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.55.4",
    "aes-js": "^3.1.2",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.4.2",
    "expo": "^51.0.32",
    "expo-apple-authentication": "~6.4.2",
    "expo-constants": "~16.0.1",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.14",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "lucide-react-native": "^0.439.0",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/aes-js": "^3.1.4",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
